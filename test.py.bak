from abc import abstractmethod,ABC

class No_of_legs(ABC):
    @abstractmethod
    def get_no_legs(self):
        pass
        

class Hen(No_of_legs):
    def get_no_legs(self):
        print(2)
class Cow(No_of_legs):
    def get_no_legs(self):
        print(4)
h=Hen()
h.get_no_legs()
c=Cow()
c.get_no_legs()